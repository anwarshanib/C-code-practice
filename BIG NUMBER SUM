#include <bits/stdc++.h>

using namespace std;
long arra[50],sum=0;
int i;
long aVeryBigSum(int n) {
    for(i=0;i<n;i++)
    {
        sum=sum+arra[i];
    }
  return sum;

}

int main()
{    

    int n;
    cin >>n;
    for(i=0;i<n;i++)
    {
        cin>>arra[i];
    }
    sum=aVeryBigSum(n);
   cout<<sum;
}

